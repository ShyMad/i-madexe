<!DOCTYPE html>
<html>
    <head>

        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        <link rel="stylesheet" href="{{ asset('src/css/backend/bootstrap/bootstrap.min.css') }}"></link> 
        <link rel="stylesheet" href="{{ asset('src/css/font-awesome/css/font-awesome.min.css') }}"></link> 
        <link rel="stylesheet" href="{{ asset('src/css/backend/data-tables/dataTables.bootstrap4.css') }}"></link> 
        <link rel="stylesheet" href="{{ asset('src/css/backend/sb-admin/sb-admin.min.css') }}"></link>  
    </head>
    <body class="fixed-nav sticky-footer bg-dark" id="page-top">
        {{ include('inc/back-nav.html.twig') }}
    <div class="content-wrapper">
        <div class="container-fluid">
        {% block body %}
        
        {% endblock %}
        </div>
    </div>
        {{ include ('inc/back-footer.html.twig')}}
        
         <!-- Scroll to Top Button-->
        <a class="scroll-to-top rounded" href="#page-top">
            <i class="fa fa-angle-up"></i>
        </a>
        {{ include('inc/back-logout.html.twig')}}
        
     <!-- Bootstrap core JavaScript-->
    <script src="{{ asset('src/js/backend/jquery/jquery.min.js') }}"></script>
    <script src="{{ asset('src/js/backend/bootstrap/bootstrap.bundle.min.js') }}"></script>
    <script src="{{ asset('src/js/backend/bootstrap/bootstrap-datepicker.js') }}"></script>
    <!-- Core plugin JavaScript-->
    <script src="{{ asset('src/js/backend/jquery-easing/jquery.easing.min.js') }}"></script>
    <!-- Page level plugin JavaScript-->
    <script src="{{ asset('src/js/backend/chart.js/Chart.min.js') }}"></script>
    <script src="{{ asset('src/js/backend/datatables/jquery.dataTables.js') }}"></script>
    <script src="{{ asset('src/js/backend/datatables/dataTables.bootstrap4.js') }}"></script>
    <!-- Custom scripts for all pages-->
    <script src="{{ asset('src/js/backend/sb-admin/sb-admin.min.js') }}"></script>
    <!-- Custom scripts for this page-->
    <script src="{{ asset('src/js/backend/sb-admin/sb-admin-datatables.min.js') }}"></script>
    <script src="{{ asset('src/js/backend/sb-admin/sb-admin-charts.min.js') }}"></script>
    {% block javascripts %}{% endblock %}
    </body>

</html>
