{% extends 'back-base.html.twig' %}

{% block title %}New Project{% endblock %}

{% block body %}
    <ol class="breadcrumb">
        <li class="breadcrumb-item">
          <a href="/backend">Dashboard</a>
        </li>
        <li class="breadcrumb-item">
          <a href="/backend/project">Project</a>
        </li>
        <li class="breadcrumb-item active">{{ tag }}</li>
    </ol>
    <h1 class="bd-title">Create new Project</h1>
    <div class="bd-exemple">
      {{ include('backend/project/_form.html.twig') }}
    </div>
    <a href="{{ path('project_index') }}">back to list</a>
